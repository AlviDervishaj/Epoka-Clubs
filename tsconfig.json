{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components": ["app/components/index.ts"],
      "@helpers": ["global/Helpers/index.ts"],
      "@utils": ["global/Utils/index.ts"],
      "@types": ["global/Types/index.ts"],
      "@framer": ["global/FramerMotion/index.ts"],
    },
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
